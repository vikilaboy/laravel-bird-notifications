{
    "name": "vikilaboy/laravel-bird-notifications",
    "description": "Bird notification channel for Laravel 11.x",
    "homepage": "https://github.com/vikilaboy/laravel-bird-notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Steenbergen",
            "email": "psteenbergen@gmail.com",
            "homepage": "https://3ws.nl",
            "role": "Developer"
        },
        {
            "name": "Victor Niculae",
            "email": "victor@niculae.net",
            "homepage": "https://niculae.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|^8.4",
        "illuminate/notifications": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/queue": "^9.0|^10.0|^11.0",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "phpunit/phpunit": "^9.5.10|^10.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Bird\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Bird\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\Bird\\BirdServiceProvider"
            ]
        }
    }
}
